  "dependencies": {
    "got": "^12.0.0",
    "got-fetch": "^5.1.6",
    "http-proxy-agent": "^7.0.0",
    "https-proxy-agent": "^7.0.0",
    "npmlog": "^7.0.1",
    "semver": "^7.5.4",
    "tar": "^6.2.0",
    "tempy": "^3.1.0"
